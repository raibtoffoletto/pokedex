{
  "name": "pokedex",
  "productName": "Pokédex",
  "description": "Simple Pokédex based on PokéAPI.co",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/raibtoffoletto/pokedex"
  },
  "keywords": [
    "Material-UI",
    "PokeAPI",
    "Pokemon",
    "PWA",
    "React"
  ],
  "author": {
    "name": "Raí Biason Toffoletto",
    "email": "rai@toffoletto.me",
    "url": "https://toffoletto.me/"
  },
  "scripts": {
    "dev": "vite",
    "dev:msw": "VITE_MSW=true vite",
    "dev:cypress": "cypress open --config-file=src/tests/cypress/config.ts",
    "run:cypress": "VITE_MSW=true vite & cypress run --headless --config-file=src/tests/cypress/config.ts",
    "build": "yarn lint && vite build",
    "lint": "tsc --noEmit && yarn eslint",
    "preview": "yarn build && vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.4.4",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@vitejs/plugin-react": "4.0.3",
    "@vitest/coverage-c8": "0.33.0",
    "cypress": "12.17.2",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "22.1.0",
    "msw": "1.2.3",
    "prettier": "3.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.6",
    "vite-plugin-pwa": "0.16.4",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.1",
    "@mui/material": "5.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
